
@model  LandingPageModel

@{
	Layout = "~/Views/Shared/_Layout.cshtml";
}
<div class="container-fluid header1 header2 minify-header appfamily-header">
	<div class="container">
		<div class="row">
			<div class="col-md-8">
				<h1>Process MS Project & Oracle Primavera files via C# REST API</h1>
				<h4>Enable your .NET apps to work with project assignments, tasks, resources, calendars, attributes, & scheduling in the cloud.</h4>
			</div>
			<div class="col-md-4 tc">
				<img class=" col-xs-12" src="../img/aspose_tasks-for-net.png" alt="Aspose.Tasks Cloud" width="320" />
			</div>
		</div>
	</div>
</div>
<div class="container-fluid productfamilypage bg-white pdb-5">
	<div class="row">
		<div class="container tc">
			<div class="clearfix">&nbsp;</div>
			<div class="clearfix">&nbsp;</div>
			<h3>Aspose.Tasks Cloud .NET APIs Includes</h3>
			<div class="clearfix">&nbsp;</div>
			<div class="clearfix">&nbsp;</div>

			<div class="col-lg-4 productfamily_box">
				<div class="productfamilytitle">
					<div class="imgblock">
						<img src="../img/aspose_conversion-app.png" alt="Aspose.Tasks Cloud .NET SDK" />
					</div>
					<a href="/tasks/conversion"><span class="spanclass">Aspose.Tasks @Model.Resources["AsposeProductFor"] Cloud<em>Conversion</em></span><em>Drop or upload your Microsoft Project file and convert it to Primavera, raster image, PDF or many other formats.</em></a>
				</div>
			</div>

		</div>
	</div>
</div>
<div class="spacer">&nbsp;</div>
<script type="text/javascript">

            window.onload = function () {
                alert("Please set APP Key and APP SID in web.config to run Aspose.Tasks Cloud .NET APIs");
            };
</script> 